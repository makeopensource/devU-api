    "color-hash": "^2.0.1",
    "devu-shared-modules": "github:UBAutograding/devu-shared#master",
    "history": "^4.10.1",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.1",
    "react-toggle": "^4.1.2",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "shortid": "^2.2.16",
    "typescript": "^4.3.2"
  },
